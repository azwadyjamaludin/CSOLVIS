{
  "name": "desktop_app",
  "productName": "CSOLVIS",
  "version": "1.0.0",
  "description": "CSOLVIS Desktop App",
  "main": "./electron.js",
  "homepage": "./",
  "dependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/preset-env": "^7.13.15",
    "@fontsource/roboto": "^4.2.3",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@sweetalert2/theme-material-ui": "^5.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "ace-builds": "^1.4.12",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "electron-builder": "^22.11.7",
    "electron-is-dev": "^2.0.0",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "insert-text-at-cursor": "^0.3.0",
    "multer": "^1.4.2",
    "node-datetime": "^2.1.2",
    "react": "^17.0.2",
    "react-ace": "^9.4.0",
    "react-bootstrap": "^1.6.0",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-swipeable-views": "^0.14.0",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "sqlite3": "^5.0.2",
    "sweetalert2": "^11.0.16",
    "uuid": "^3.4.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "concurrently \"BROWSER=none\" \"npm run server-start\" \"wait-on http://localhost:3000 && npm run electron-dev\"",
    "build": "react-scripts build",
    "react-start": "react-scripts start",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron": "wait-on tcp:3000 && electron .",
    "electron-dev": "node scripts/dev.js scripts/start.js",
    "electron-pack": "electron-builder -c.extraMetadata.main='./electron.js'",
    "preelectron-pack": "npm run react-build && npm run server-build",
    "server-build": "babel server --out-dir build-server",
    "server-start": "nodemon --watch server --exec babel-node server/server.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "concurrently": "^6.0.2",
    "electron": "^12.0.6",
    "log4js": "^0.6.38",
    "nodemon": "^2.0.7",
    "piping": "*",
    "socket.io": "^4.0.1",
    "wait-on": "^5.3.0"
  },
  "build": {
    "appId": "azwadyInc.csolvis",
    "files": [
      "build/**/**/*",
      "build-server/**/**/*",
      "node_modules/**/*",
      "./electron.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
